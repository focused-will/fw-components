{
  "name": "@focused-will/components",
  "version": "0.3.4",
  "description": "Components for WoW apps",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/focused-will/fw-components.git"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "peers:install": "npm install --no-save react react-dom",
    "peers:uninstall": "rm -rf node_modules/react node_modules/react-dom",
    "test": "jest",
    "prebuild": "npm run peers:uninstall",
    "build": "rollup -c",
    "postbuild": "npm run peers:install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "import": "node --loader ts-node/esm scripts/import.ts import ",
    "bulk-import": "node --loader ts-node/esm scripts/import.ts bulk",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@radix-ui/colors": "^0.1.8",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@stitches/react": "^1.2.8",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack4": "^6.5.12",
    "@storybook/manager-webpack4": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@swc/core": "^1.3.1",
    "@swc/jest": "^0.2.22",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.0.2",
    "@types/react": "^18.0.20",
    "@types/yargs": "^17.0.12",
    "babel-loader": "^8.2.5",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "puppeteer": "^18.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-swc3": "^0.6.0",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3",
    "yargs": "^17.5.1"
  },
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.0.0",
    "react-xarrows": "^2.0.2",
    "zod": "^3.19.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
